<form method="post" action="{{ path('cedula_delete', {'id': cedula.rut}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cedula.rut) }}">
    <button class="btn">Delete</button>
</form>
