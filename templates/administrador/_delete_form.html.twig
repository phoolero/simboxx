<form method="post" action="{{ path('administrador_eliminar', {'id': administrador.id}) }}"  onsubmit="return confirm('¿Está seguro de borrar este Administrador?');" style="display:contents;">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ administrador.id) }}">
    <button class="btn btn-danger"><i class="fas fa-trash text-white"></i></button>
</form>
